{
	"extends": "airbnb",
	"rules": {
		"no-console": "off",
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"no-tabs": "off",
		"indent": [ 2, "tab" ],
		"react/jsx-indent": [ 2, "tab" ],
		"react/jsx-indent-props": [ 2, "tab" ],
		"react/forbid-prop-types": "off",
		"comma-dangle": ["error", "never"],
		"jsx-a11y/no-static-element-interactions": "off"
	},
	"parser": "babel-eslint"
}
